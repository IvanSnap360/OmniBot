<launch>


  <rosparam command="load" file="$(find omnibot_gazebo)/config/gazebo_pids.yaml" />


  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="paused" value="false" />
    <arg name="use_sim_time" value="true" />
    <arg name="gui" value="false" />
    <arg name="recording" value="false" />
    <arg name="debug" value="false" />
    <arg name="verbose" value="false" />
    <arg name="world_name" value="$(find omnibot_gazebo)/worlds/turtlebot3_worlds/turtlebot3_world.world" />
  </include>

  <node pkg="gazebo_ros" type="spawn_model" name="omnibot_model_spawner"
    args="-urdf -param robot_description -x -2 -z 0.040 -model omnibot_robot" respawn="false"
    output="screen" />


</launch>